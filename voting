import java.util.Scanner;
class vote{
public static void main(String [] args){
int a,shrt;
Scanner sc = new Scanner(System.in);
System.out.println("enter age;");
 a=sc.nextInt();
if(a>=18)
{
System.out.println("valid");
}
else
{
 shrt=(18-a);
System.out.println("not valid " + shrt + "years more");
}}}
